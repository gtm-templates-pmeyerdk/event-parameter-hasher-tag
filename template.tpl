___SANDBOXED_JS_FOR_WEB_TEMPLATE___

// APIs
const callInWindow = require("callInWindow");
const createQueue = require("createQueue");
const dataLayerPush = createQueue("dataLayer");
const injectScript = require("injectScript");
const logToConsole = require("logToConsole");
const permission = require("queryPermission");

// Global vars
const _hashScript = "https://cdnjs.cloudflare.com/ajax/libs/js-sha256/0.9.0/sha256.min.js";
const _logPrefix = "[GTM | Event Parameter Hasher] ";

// Functions
const log = message => {
  logToConsole(_logPrefix + message);
};

const onInjectSuccess = () => {
  let event = {};
  let eventData = data.eventData;
  
  if (data.eventName) {
    event.event = data.eventName;
  }

  for (var i = 0; i < eventData.length; i += 1) {
    const key = eventData[i].key;
    const hash = eventData[i].hash;
    let value = eventData[i].value;

    if (hash === true) {
      value = callInWindow("sha256", value);
    }
    event[key] = value;
  }

  if (permission("access_globals", "readwrite", "dataLayer")) {
    dataLayerPush(event, data.gtmOnSuccess, data.gtmOnFailure);
  }
  else {
    log("Error: Cannot push event to the data layer without permission.");
    data.gtmOnFailure();
  }
};

// Run it all
log("data =" + data);

if (permission("inject_script", _hashScript)) {
  injectScript(_hashScript, onInjectSuccess, data.gtmOnFailure);
} else {
  log("Error: Cannot inject external SHA256 script without permission.");
  data.gtmOnFailure();
}

// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


